{
  "name": "patrick-grace-wedding",
  "version": "0.0.0",
  "description": "Patrick & Grace Wedding Site",
  "main": "handler.js",
  "scripts": {
    "client": "node client/scripts/start.js",
    "server": "NODE_ENV=dev DB_ENDPOINT=http://localhost:8000 sls offline start",
    "dev": "concurrently  --kill-others-on-fail \"npm run server\" \"npm run client\" ",
    "build": "node client/scripts/build.js",
    "lint": "eslint .",
    "preflight": "npm run lint && npm run typecheck && npm run test",
    "release": "node release.js",
    "test": "jest",
    "typecheck": "flow .",
    "butts": "uuid"
  },
  "author": "Patrick Corbett <patrickmacorbett@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.5",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-crowdlab": "^1.0.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.6.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-crowdlab": "^2.0.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "evergreen-ui": "^4.8.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.76.0",
    "fs-extra": "3.0.1",
    "fullstack-serverless": "^0.5.6",
    "html-webpack-plugin": "2.29.0",
    "jasmine-expect": "^3.8.3",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "jest-junit": "^3.6.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-styled-components": "^5.0.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "raw-loader": "^1.0.0",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.4",
    "react-phone-input-2": "^2.4.1",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^4.3.1",
    "react-s3-uploader": "^4.8.0",
    "react-select": "^2.1.2",
    "resolve": "1.6.0",
    "serverless-cloudfront-invalidate": "^1.2.1",
    "serverless-offline": "^3.25.6",
    "serverless-print-dots": "^1.0.4",
    "serverless-webpack": "^5.2.0",
    "simple-git": "^1.107.0",
    "style-loader": "0.19.0",
    "styled-components": "^4.1.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "uuid": "^3.3.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/client/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/client/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "proxy": "http://localhost:4000/",
  "dependencies": {
    "aws-sdk": "^2.272.1",
    "axios": "^0.18.0",
    "dynamodb-data-types": "^3.0.1",
    "flow-aws-lambda": "^1.0.3",
    "ramda": "^0.25.0"
  }
}
